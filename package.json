{
  "name": "notion-recursive-scraper",
  "version": "1.0.0",
  "description": "A sophisticated recursive scraper for Notion pages with hierarchical organization",
  "main": "main-cluster.js",
  "scripts": {
    "start": "node main-cluster.js",
    "test": "jest --projects unit integration",
    "test:unit": "jest --selectProject=unit",
    "test:integration": "jest --selectProject=integration",
    "test:e2e": "jest --selectProject=e2e",
    "test:watch": "jest --watch --projects unit",
    "test:coverage": "jest --coverage --projects unit integration",
    "test:all": "jest --projects unit integration e2e",
    "test:legacy": "node tests/test-integration.js",
    "test:legacy:worker": "node tests/test-worker.js",
    "test:legacy:cluster": "node tests/test-cluster.js",
    "test:legacy:crash": "node tests/test-worker-crash.js",
    "test:legacy:cookies": "node tests/test-cookie-propagation.js"
  },
  "keywords": [
    "notion",
    "scraper",
    "puppeteer",
    "recursive",
    "web-scraping"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cli-progress": "^3.12.0",
    "jsdom": "^23.0.0",
    "puppeteer": "^24.30.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "jest-extended": "^7.0.0",
    "mock-fs": "^5.5.0",
    "nock": "^14.0.10"
  }
}
